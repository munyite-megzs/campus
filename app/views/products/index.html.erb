<h1>All Products</h1>

<ul id="products">
  <% @products.each do |product| %>
    <li>
    <div class="card-panel">
      <%= link_to product.productTitle, product_path(product), class: 'product-title-link' %>
    </div>
    </li>
  <% end %>
</ul>
<%= link_to "Add Product" , new_product_path, class: 'new-product'%>